{
  "name": "nuxt-express",
  "version": "1.0.0",
  "private": true,
  "main":"nuxt.config.js",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && mkdir -p ./build/bin && cp -r node_modules/.bin/* ./build/bin",
    "start": "ls -la && cd build && ls -la &&  && ls -la ./bin &&  ./bin/nuxt start",
    "generate": "nuxt generate",
    "lint": "yarn lint:js",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxt/http": "latest",
    "express": "latest",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
  }
}
